{
  "name": "vue-summit-ssr-webpack-router",
  "version": "1.0.0",
  "description": "Universal Vue SSR with Webpack and Vue Router",
  "author": "Erick Eduardo Petrucelli <erick.petrucelli@fatectq.edu.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/erickpetru/vue-summit-ssr.git"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack -r esm --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack -r esm --config build/webpack.server.config.js --progress --hide-modules",
    "dev": "npm run build && node -r esm server.js"
  },
  "dependencies": {
    "esm": "^3.0.80",
    "express": "^4.16.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
